<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lh.demo.dao.UserDao">
    <select id="getUser" resultType="com.lh.demo.entity.User">
    select
    userId as Id,
    userName as name,
    userAge as age,
    userAddress as address
    from user
   </select>
</mapper>